package com.jin.var;

public class Varriable_09 {

	public static void main(String[] args) {
		
		int intNum = 0;
		
		System.out.println(intNum++);
		System.out.println(intNum++);
		System.out.println(intNum++);
		System.out.println(intNum++);
		System.out.println(intNum++);
		// 01234 로 출력
		
	}
}
