package com.jin.var;

public class Varriable_02 {
	
	public static void main(String[] args) {
		
		int intNum1 = 0; //기본형  
		// 정수형 변수 선언과 clear
		// 정수형 변수 intNum1, intNum2 생상
		
		Integer intNum2 = 0; //확장형
		
		float floatNum1 = 0.0f;
		Float floatNum2 = 0.0F;
		// 실수형 변수 floatNum1, floatNum2 생성
		// 기억장치를 소모하는 용량이 double 보다 작음
		// 대신 정밀도는 double 보다 낮음
		
		long longNum1 = 0l;
		Long longNum2 = 0L;
		// 정수형 변수 longNum1, longNum2 생성
		// int, Integer 형보다 기억장치 소모 용량이 크다
		// int 형은 최대 2의 32승만큼 공간 자치
		// long 형은 최대 2의 64승만큼 공간
		
		double douNum1 = 0.0;
		Double douNum2 = 0.0;
		// 실수형 변수 선언과 clear
		// 실수형 변수 douNum1, douNum2 생성
		
		String strName1 =  "";
		String strName2 = null;		
		// 문자열 형 변수 strName 을 생성
		// 문자열 형 변수를 선언하고 clear
		
		intNum1 = 30;
		intNum2 = 40;
		
		floatNum1 = 30.0F;
		floatNum2 = 40.2f;
		
		floatNum1 = 30;
		
		longNum1 = 30l;
		longNum2 = 40L;
		
		douNum1 = 45.0;
		douNum2 = 55.0;
		
		strName1 = "홍길동";
		strName2 = "이몽룡";
		
		
		
		
		
	}
	
}